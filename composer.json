{
    "name": "deploy-dog/slavedriver",
    "description": " A PHP based cron job runner. Manage, monitor, log and react to problems with your scheduled tasks.",
    "license": "MIT",
    "autoload": {
        "psr-0" : {
            "deploydog\\Slavedriver" : "src"
        }
    },
    "require": {
        "php": "^7.0",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "mtdowling/cron-expression": "^1.2",
        "malkusch/lock": "^1.0",
        "cocur/slugify": "^2.5",
        "phossa2/event": "^2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "suggest": {
        "matthiasmullie/scrapbook": "Caching adapters compatible with Slavedriver.",
        "monolog/monolog": "Compatible loggers."
    }
}
